{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Launch Chrome",
			"type": "chrome",
			"request": "launch",
			"url": "http://localhost:4200",
			"webRoot": "\",
			"sourceMaps": true,
			"sourceMapPathOverrides": {
				"webpack:/*": "\/*",
				"/./*": "\/*",
				"/src/*": "\/src/*",
				"/*": "*",
				"/./~/*": "\/node_modules/*"
			}
		},
		{
			"name": "Attach to Chrome",
			"type": "chrome",
			"request": "attach",
			"port": 9222,
			"webRoot": "\",
			"sourceMaps": true
		}
	]
}

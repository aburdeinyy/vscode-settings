{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Launch Chrome",
			"type": "chrome",
			"request": "launch",
			"url": "http://localhost:4200",
			"webRoot": "${workspaceFolder}",
			"sourceMaps": true,
			"sourceMapPathOverrides": {
				"webpack:/*": "${webRoot}/*",
				"/./*": "${webRoot}/*",
				"/src/*": "${webRoot}/src/*",
				"/*": "*",
				"/./~/*": "${webRoot}/node_modules/*"
			}
		},
		{
			"name": "Attach to Chrome",
			"type": "chrome",
			"request": "attach",
			"port": 9222,
			"webRoot": "${workspaceFolder}",
			"sourceMaps": true
		}
	]
}